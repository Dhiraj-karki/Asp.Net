@model List<dhiraj.Models.doctor>
@{
    ViewBag.Title = "index1";
    int sn = 0;
}

<style>
    table {
        background-color: black;
        color: white;
    }
</style>
<a href="@Url.Action("dhiraj1")"> Add new record</a>
<h2> index</h2>
<table class="table" border="1">
    <tr>
        <th> ID</th>
        <th> Name</th>
        
        <th> contact</th>

    </tr>
    @foreach (var data in Model)
    {
        <tr>
            <td>@(++sn) </td>
            <td>@(data.d_name) </td>
            <td>@(data.contact) </td>
            <td><a href="@Url.Action("edit1", new {id=data.d_id})">Edit<</a></td>
            <td><a href="@Url.Action("deletedata",new {d_id=data.d_id })">Delete</a></td>
        </tr>
    }
</table>
